{
  "name": "google-calendar-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --max-warnings=0 src",
    "preview": "vite preview",
